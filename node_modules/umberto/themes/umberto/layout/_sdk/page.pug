extends ../base

include ./_mixin/_sdk-samples.pug

block content
	if ( page.presetVersion )
		script( src=`//cdn.ckeditor.com/${ page.ckeditorVersion }/${ page.presetVersion }/ckeditor.js` )
	else
		script( src=`../../../vendors/${ page.ckeditorVersion }/ckeditor/ckeditor.js` )

	script( src='assets/icons.js' )

	!= content

	if ( page.sdkSamples )
		+sdkSamples( page.sdkSamples )

block navtree
	.side-navigation.side-navigation--sdk
		.side-navigation__wrapper
			nav.side-navigation__inner
				ul.tree.sdk-tree
					include ./_partial/navtree.pug
