mixin sdkNavTreeItem( data )
	- const _class = page && page.path.endsWith( '/' + data.path ) ? 'tree__item__wrapper tree__item__wrapper--active' : 'tree__item__wrapper';
	- const itemTitle = data.title;
	li
		a( href=( data.path ) )
			div( class=_class )
				span( class="tree__item--sdk tree__item__text" ) #{ itemTitle }
