include ../_mixin/_api-tree-item.pug

.side-navigation
	.side-navigation__wrapper
		nav.side-navigation__inner.side-navigation__inner--api
			if projectLocals && projectLocals.apiTree
				ul( class="tree api-tree" )
						each node in projectLocals.apiTree.getTree().getChildren()
							+treeItem( node )
				div.api-tree__filter
					p
						input( type="checkbox" id="p1" value="public" )
						label( for="p1" ) Show private items
