include ../_mixin/_method

if isNonEmptyArray( data.methods )
	h2( id="methods" class="hidden-loading" ) Methods
	if isNonEmptyArray( data.staticMethods)
		if isNonEmptyArray( data.instanceMethods)
			ul( class="collapsing-list collapsing-list--filtered hidden-loading" data-type="methods" )
				each m in data.instanceMethods
					+method( m )

		h4( class="hidden-loading" ) Static methods
		ul( class="collapsing-list collapsing-list--filtered hidden-loading" data-type="methods" )
			each m in data.staticMethods
				+method( m )

	else
		ul( class="collapsing-list collapsing-list--filtered hidden-loading" data-type="methods" )
			each m in data.methods
				+method( m )
